{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "build/**"]
    },
    "build:css": {
      "outputs": ["public/**/*.css"]
    },
    "watch:css": {
      "outputs": ["public/**/*.css"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": true
    }
  }
}
