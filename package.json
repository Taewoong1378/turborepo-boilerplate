{
  "name": "turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "main": "concurrently \"turbo run watch:css\" \"turbo run dev --parallel --filter=main...\"",
    "main-build": "turbo run build:css && turbo run build --parallel --filter=main...",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "tsc:check": "tsc -p tsconfig.json"
  },
  "lint-staged": {
    "apps/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "packages/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@swc/core": "^1.3.96",
    "@types/lodash-es": "^4.17.11",
    "@types/node": "^18.18.9",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^8.3.4",
    "@types/webpack-env": "^1.18.4",
    "autoprefixer": "^10.4.16",
    "compression-webpack-plugin": "^10.0.0",
    "concurrently": "^8.2.2",
    "eslint-prettier-config-custom": "workspace:*",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.31",
    "postcss-import": "^14.1.0",
    "tailwind-base": "workspace:*",
    "ts-node": "10.9.1",
    "tsconfig": "workspace:*",
    "turbo": "^1.10.16",
    "typescript": "^4.9.5",
    "typescript-plugin-css-modules": "^5.0.2"
  },
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "packageManager": "pnpm@7.13.4",
  "dependencies": {
    "@vercel/analytics": "^0.1.11",
    "classnames": "^2.3.2",
    "design-system": "workspace:*",
    "framer-motion": "^8.5.5",
    "jotai": "^2.5.1",
    "jotai-immer": "^0.2.0",
    "jotai-optics": "^0.3.1",
    "lodash-es": "^4.17.21",
    "next": "^13.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.5",
    "shared-utils": "workspace:*",
    "uuid": "^8.3.2",
    "yup": "^1.3.2"
  }
}
